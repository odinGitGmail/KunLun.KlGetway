        }
      ],
      "ServerAddresses": [ "http://192.168.202.132:8848" ],
      "Namespace": "",
      "ConfigUseRpc": false,
      "NamingUseRpc": false
    }
  },
  "nacos": {
    //"EndPoint": "sub-domain.aliyun.com:8080",//和上面的解释一样，最好留空，不然很容易报错。
